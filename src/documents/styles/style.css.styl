.boxes
  font-weight: bold
.header
  text-align:center

.row
  margin-bottom:2px
  text-align:center

.alert
.panel
  text-align:left
  border-radius:8px
  margin-top:5px

.btn
  margin-top:5px

h6
  margin-left:5px
  margin-top:13px
  font-family:sans-serif
  text-align:left
  line-height:5px

.col-md-6
  padding-left:25px
  padding-right:25px

.col-xs-4,
.col-xs-3,
.col-xs-6
  padding-left:1px
  padding-right:1px

/* Research Bars */
.progress
  width: 100%
  height: 1.3em
.progress-wrap
  background: #D9534F
  overflow: hidden
  position: relative
.researchProgress
.researchingComment
  line-height:39px
  color:white

.info
  overflow:hidden
  max-height:200px

.info p
  text-align:left

.well
  padding-top:0
.btn
  border-radius: 8px
  font-weight: bold
  .btn-warning
    color:#333
  .iron
    background-color: iron
  .copper
    background-color: copper
  .silver
    background-color: silver
.btn-group > span.btn:first-of-type
  border-right:3px solid white
.hudList
  display: inline
  list-style-type: none
  text-align: justify
.hudNav :not(first-child) li
    float: left
    list-style-type: none
    margin-left: 5px